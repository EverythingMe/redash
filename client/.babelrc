{
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": ["@babel/plugin-transform-async-to-generator", "@babel/plugin-transform-arrow-functions"],
        "useBuiltIns": "usage",
        "corejs": "3"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-object-assign",
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  }
}
